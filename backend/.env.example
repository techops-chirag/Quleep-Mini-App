# Express server port
PORT=5000

# PostgreSQL connection string
# Default local DB setup (user=postgres, password=postgres, db=quleepdb)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/quleepdb

# Allowed frontend origins for CORS (comma separated)
ALLOWED_ORIGINS=http://localhost:3000

# Optional: JWT secret (if implementing bonus authentication)
JWT_SECRET=supersecretkey
